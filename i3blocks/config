interval=1

[keyboard]
command=~/.config/i3blocks/scripts/keyboard.fish
interval=persist

[sound]
command=~/.config/i3blocks/scripts/sound.fish
interval=persist

[volume]
command=~/.config/i3blocks/scripts/volume.fish
interval=persist
markup=pango

[network]
command=~/.config/i3blocks/scripts/network.fish
interval=5
markup=pango

[date]
command=date '+ %H:%M %d %b '
interval=10

[resources]
command=~/.config/i3blocks/scripts/resources.fish
interval=10
